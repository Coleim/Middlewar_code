<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
	PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
	"http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

	<!-- Default page to serve -->
	<welcome-file-list>
		<welcome-file>MainModule.html</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>CharacterService</servlet-name>
		<servlet-class>middlewar.server.CharacterServiceImpl</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CharacterService</servlet-name>
		<url-pattern>/middlewar.MainModule/CharacterService</url-pattern>
	</servlet-mapping>

</web-app>
